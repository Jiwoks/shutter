msgid "Capture with selection"
msgstr "Aufnahme mit Auswahl"

#: gscrot.pl:742
msgid "Quit"
msgstr "Beenden"

#: gscrot.pl:801
msgid "Delete file(s)"
msgstr "Lösche Datei(en)"

#: gscrot.pl:809
msgid "Copy file(s) to clipboard"
msgstr "Kopiere Datei(en) in die Zwischenablage"

#: gscrot.pl:817
msgid "Print file(s)"
msgstr "Drucke Datei(en)"

#: gscrot.pl:846
msgid "will be printed"
msgstr "wird gedruckt"

#: gscrot.pl:853
msgid "deleted"
msgstr "gelöscht"

#: gscrot.pl:865
msgid "copied to clipboard"
msgstr "in die Zwischenablage kopiert"

#: gscrot.pl:879
msgid "All screenshots deleted"
msgstr "Alle Aufnahmen dieser Sitzung gelöscht"

#: gscrot.pl:890
msgid "Printing all screenshots"
msgstr "Alle Aufnahmen werden gedruckt"

#: gscrot.pl:902
msgid "Do you want to overwrite the existing settings?"
msgstr "Möchten Sie die bestehenden Einstellungen überschreiben?"

#: gscrot.pl:912
msgid "There are no stored settings"
msgstr "Es existieren keine gesicherten Einstellungen"

#: gscrot.pl:921
#: gscrot.pl:933
msgid "Settings could not be saved"
msgstr "Einstellungen konnten nicht gespeichert werden"

#: gscrot.pl:936
msgid "Settings saved successfully"
msgstr "Einstellungen erfolgreich gespeichert"

#: gscrot.pl:942
#: gscrot.pl:944
